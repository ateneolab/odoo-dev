				<link href="/theme_zap/static/src/less/customize_modal.less" rel="stylesheet" type="text/less"/>
			</xpath>
		</template>

		<template id="assets_editor" inherit_id="website_less.assets_editor" name="Editor assets">
			<xpath expr="." position="inside">
				<!-- <script type="text/javascript" src="/theme_common/static/src/js/s_tabs.js"></script> -->
				<script type="text/javascript" src="/theme_common/static/src/js/s_collapse.js"></script>
			</xpath>
		</template>

	</data>
</openerp>