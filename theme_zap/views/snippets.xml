<?xml version="1.0" encoding="utf-8" ?>
<openerp>
  <data>

    <template id="s_selection" inherit_id="website_less.snippets">


<!-- STRUCTURE
     ================================================== -->
      
      <!-- Carousel & FW -->
      <xpath expr="//div[@id='snippet_structure']" position="inside">
        <t t-snippet="theme_common.s_three_columns_carousel" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_three_columns_carousel.gif"/>
        <t t-snippet="theme_common.s_carousel_boxed" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_carousel_boxed.gif"/>
        <t t-snippet="theme_common.s_four_columns_fw" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_four_columns_fw.gif"/>
        <t t-snippet="theme_common.s_image_text_fw" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_image_text_fw.gif"/>
        <t t-snippet="theme_common.s_text_block_image_fw" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_text_block_image_fw.gif"/>
        <t t-snippet="theme_common.s_text_image_fw" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_text_image_fw.gif"/>
        <t t-snippet="theme_common.s_images_captions_fw" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_images_captions_fw.gif"/>
      </xpath>

      <!-- Banner -->
      <xpath expr="//div[@id='snippet_structure']//div[@name='Banner']" position="after">
        <t t-snippet="theme_common.s_banner_2" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_banner_2.gif"/>
      </xpath>

      <!-- Page Header -->
      <xpath expr="//div[@id='snippet_structure']//div[@name='Title']" position="after">
        <t t-snippet="theme_common.s_page_header" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_page_header.gif"/>
      </xpath>

      <!-- Features Circle -->
      <xpath expr="//div[@id='snippet_structure']//div[@name='Features']" position="after">
        <t t-snippet="theme_common.s_features_circle" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_features_circle.gif"/>
        <t t-snippet="theme_common.s_numbers" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_numbers.gif"/>
      </xpath>


<!-- CONTENT
     ================================================== -->

      <!-- LABEL -->
      <xpath expr="//div[@id='snippet_content']" position="inside">
        <t t-snippet="theme_common.s_label" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_label.gif"/>
      </xpath>


<!-- FEATURES
     ================================================== -->

     <!-- References -->
      <xpath expr="//div[@id='snippet_feature']//div[@name='References']" position="after">
        <t t-snippet="theme_common.s_references_2" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_references_2.gif"/>
      </xpath>

      <!-- Quotes Slider -->
      <xpath expr="//div[@id='snippet_feature']//div[@name='Quotes Slider']" position="after">
        <t t-snippet="theme_common.s_quotes_slider_2" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_quotes_slider_2.gif"/>
      </xpath>

      <!-- Quotes Slider -->
      <xpath expr="//div[@id='snippet_feature']//div[@name='Feature Grid']" position="after">
        <t t-snippet="theme_common.s_features_grid_circle" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_features_grid_circle.gif"/>
      </xpath>

      <xpath expr="//div[@id='snippet_feature']" position="inside">
        <t t-snippet="theme_common.s_collapse" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_collapse.gif"/>
        <t t-snippet="theme_common.s_medias_list" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_medias_list.gif"/>
        <t t-snippet="theme_common.s_news_carousel" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_news_carousel.gif"/>
        <t t-snippet="theme_common.s_process_steps_2" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_process_steps_2.gif"/>
<!--    <t t-snippet="theme_common.s_tabs" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_tabs.gif"/> -->
        <t t-snippet="theme_common.s_team_profiles_2" t-thumbnail="/theme_zap/static/src/img/snippets_thumbs/s_team_profiles_2.gif"/>
      </xpath>

    </template>

  </data>
</openerp>