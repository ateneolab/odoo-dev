<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>

	<template name="Panel Extended" id="s_panel_extended" inherit_id="website_less.snippets">
		<xpath expr="//div[contains(@class, 's_panel')]" position="attributes">
			<attribute name="class"> s_panel panel panel-beta</attribute>
		</xpath>
		<xpath expr="//div[contains(@class, 's_panel')]//h3" position="replace">
			<h4 class="mt0 mb0"><i class="fa fa-star fa-1x text-muted"></i>Feature Title</h4>
		</xpath>
	</template>

	</data>
</openerp>